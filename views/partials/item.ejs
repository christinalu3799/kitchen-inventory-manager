<a href="/inventory/" class="back-btn">Back</a>

<% let c = categories.find(cat => cat.category === category) %> 
<h1><%= c.title %> </h1>

<div class="items-container">
    <% for (let i = 0; i < items.length; i++) { %> 
        <a href='/inventory/<%= category %>/<%= items[i].id %> '>
            <div class="item-card">
                <h2><%= items[i].item %></h2>
                <div class="item-img">
                    <img src=<%= items[i].imageURL %> alt=<%= items[i].item %>> 
                </div>
            </div>
        </a> 
    <% } %> 
</div>
